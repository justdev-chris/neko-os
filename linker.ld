OUTPUT_FORMAT(elf32-i386)
ENTRY(kernel_main)

SECTIONS {
    . = 1M;
    
    .multiboot : {
        *(.multiboot)
    }
    
    .text : ALIGN(4K) {
        *(.text)
    }
    
    .rodata : ALIGN(4K) {
        *(.rodata)
    }
    
    .data : ALIGN(4K) {
        *(.data)
    }
    
    .bss : ALIGN(4K) {
        *(COMMON)
        *(.bss)
    }
}
